{
  "compilerOptions": {
  "allowJs": true,
  "target": "es6",
  "lib": [
    "es2017",
    "es5",
    "es6",
    "es2015.promise",
    "dom"
  ],
  "sourceMap": true,
  "noImplicitAny": true,
  "module": "commonjs",
  "strictPropertyInitialization": false,
  "resolveJsonModule": true,
  "esModuleInterop": true
  },
  "include": [
    "src/**"
  ],
  "exclude": [
    "node_modules"
  ]
}
